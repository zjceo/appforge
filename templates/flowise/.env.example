# Flowise Configuration

# Puerto para acceder a Flowise
FLOWISE_PORT=3000

# Autenticación (opcional)
FLOWISE_USERNAME=
FLOWISE_PASSWORD=

# Rutas de almacenamiento
DATABASE_PATH=/home/node/.flowise
APIKEY_PATH=/home/node/.flowise
SECRETKEY_PATH=/home/node/.flowise

# Logging
LOG_LEVEL=info
DEBUG=false

# Secret Key (opcional, si no se proporciona se genera automáticamente)
FLOWISE_SECRETKEY_OVERWRITE=

# Dependencias de herramientas
TOOL_FUNCTION_BUILTIN_DEP=
